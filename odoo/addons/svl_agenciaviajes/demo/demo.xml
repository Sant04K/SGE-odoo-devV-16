
<odoo>
    <data>
        <record id="destino_espana" model="svl_agenciaviajes.destino">
            <field name="continente">Europa</field>
            <field name="pais">España</field>
            <field name="name">Madrid</field>
            <field name="imagen" type="base64" file="svl_agenciaviajes/demo/images/madrid.jpg"/>
        </record>

        <record id="destino_francia" model="svl_agenciaviajes.destino">
            <field name="continente">Europa</field>
            <field name="pais">Francia</field>
            <field name="name">París</field>
            <field name="imagen" type="base64" file="svl_agenciaviajes/demo/images/paris.jpg"/>
        </record>

        <record id="destino_italia" model="svl_agenciaviajes.destino">
            <field name="continente">Europa</field>
            <field name="pais">Italia</field>
            <field name="name">Roma</field>
            <field name="imagen" type="base64" file="svl_agenciaviajes/demo/images/roma.jpg"/>
