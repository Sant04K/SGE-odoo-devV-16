            <field name="imagen" type="base64" file="svl_agenciaviajes/demo/images/berlin.jpg"/>
        </record>

        <record id="destino_grecia" model="svl_agenciaviajes.destino">
            <field name="continente">Europa</field>
            <field name="pais">Grecia</field>
            <field name="name">Atenas</field>
            <field name="imagen" type="base64" file="svl_agenciaviajes/demo/images/atenas.jpg"/>
        </record>

        <record id="destino_eeuu_ny" model="svl_agenciaviajes.destino">
            <field name="continente">Norteamérica</field>
            <field name="pais">Estados Unidos</field>
            <field name="name">Nueva York</field>
            <field name="imagen" type="base64" file="svl_agenciaviajes/demo/images/nueva_york.jpg"/>
        </record>

        <record id="destino_eeuu_ca" model="svl_agenciaviajes.destino">
            <field name="continente">Norteamérica</field>
            <field name="pais">Estados Unidos</field>
            <field name="name">Los Ángeles</field>
            <field name="imagen" type="base64" file="svl_agenciaviajes/demo/images/los_angeles.jpg"/>
        </record>

        <record id="destino_mexico" model="svl_agenciaviajes.destino">
            <field name="continente">América</field>
            <field name="pais">México</field>
            <field name="name">Ciudad de México</field>
            <field name="imagen" type="base64" file="svl_agenciaviajes/demo/images/mexico.jpg"/>
        </record>

        <record id="destino_argentina" model="svl_agenciaviajes.destino">
            <field name="continente">América</field>
            <field name="pais">Argentina</field>
            <field name="name">Buenos Aires</field>
            <field name="imagen" type="base64" file="svl_agenciaviajes/demo/images/buenos_aires.jpg"/>
        </record>

        <record id="destino_japon" model="svl_agenciaviajes.destino">
            <field name="continente">Asia</field>
            <field name="pais">Japón</field>
            <field name="name">Tokio</field>
            <field name="imagen" type="base64" file="svl_agenciaviajes/demo/images/tokio.jpg"/>
        </record>

        <record id="destino_china" model="svl_agenciaviajes.destino">
            <field name="continente">Asia</field>
            <field name="pais">China</field>
            <field name="name">Pekín</field>
            <field name="imagen" type="base64" file="svl_agenciaviajes/demo/images/pekin.jpg"/>
        </record>

        <record id="temporada_alta" model="svl_agenciaviajes.temporada">
            <field name="state">alta</field>
        </record>

        <record id="temporada_baja" model="svl_agenciaviajes.temporada">
            <field name="state">baja</field>
        </record>

        <record id="temporada_oportunidad" model="svl_agenciaviajes.temporada">
            <field name="state">oportunidad</field>
        </record>

        <record id="cliente_demo_ana" model="svl_agenciaviajes.cliente">
            <field name="name">Ana</field>
            <field name="apellidos">García López</field>
            <field name="email">ana.garcia@demo.com</field>
            <field name="telefono">+34 600 111 222</field>
            <field name="destino_ids" eval="[(6, 0, [ref('destino_espana'), ref('destino_italia'), ref('destino_francia')])]"/>
        </record>

        <record id="cliente_demo_john" model="svl_agenciaviajes.cliente">
            <field name="name">John</field>
            <field name="apellidos">Smith</field>
            <field name="email">john.smith@demo.com</field>
            <field name="telefono">+1 212 555 0199</field>
            <field name="destino_ids" eval="[(6, 0, [ref('destino_eeuu_ny'), ref('destino_japon')])]"/>
        </record>

        <record id="cliente_demo_maria" model="svl_agenciaviajes.cliente">
            <field name="name">María</field>
            <field name="apellidos">Rodríguez Pérez</field>
            <field name="email">maria.rodriguez@demo.com</field>
            <field name="telefono">+52 55 1234 5678</field>
            <field name="destino_ids" eval="[(6, 0, [ref('destino_mexico'), ref('destino_argentina')])]"/>
        </record>

        <record id="cliente_demo_kenji" model="svl_agenciaviajes.cliente">
            <field name="name">Kenji</field>
            <field name="apellidos">Tanaka</field>
            <field name="email">kenji.tanaka@demo.com</field>
            <field name="telefono">+81 3 1234 5678</field>
            <field name="destino_ids" eval="[(6, 0, [ref('destino_grecia'), ref('destino_china'), ref('destino_alemania')])]"/>
        </record>

        <record id="cliente_demo_laura" model="svl_agenciaviajes.cliente">
            <field name="name">Laura</field>
            <field name="apellidos">Dubois</field>
            <field name="email">laura.dubois@demo.com</field>
            <field name="telefono">+33 1 45 67 89 00</field>
            <field name="destino_ids" eval="[(6, 0, [ref('destino_eeuu_ca')])]"/>
        </record>

        <record id="viaje_demo_roma" model="svl_agenciaviajes.viaje">
            <field name="name">Escapada Romántica a Roma</field>
            <field name="precio_base">1000.0</field>
            <field name="destino_id" ref="destino_italia"/> 
            <field name="cliente_id" ref="cliente_demo_kenji"/> 
            <field name="temporada_id" ref="temporada_alta"/> 
            <field name="fecha_inicio" eval="(datetime.now() - relativedelta(days=15)).strftime('%Y-%m-%d')"/>
            <field name="fecha_fin" eval="(datetime.now() - relativedelta(days=10)).strftime('%Y-%m-%d')"/>
        </record>

        <record id="viaje_demo_tokio" model="svl_agenciaviajes.viaje">
            <field name="name">Aventura en Tokio</field>
            <field name="precio_base">2000.0</field>
            <field name="destino_id" ref="destino_japon"/>
            <field name="cliente_id" ref="cliente_demo_maria"/>
            <field name="temporada_id" ref="temporada_oportunidad"/>
            <field name="fecha_inicio" eval="(datetime.now() - relativedelta(days=2)).strftime('%Y-%m-%d')"/>
            <field name="fecha_fin" eval="(datetime.now() + relativedelta(days=5)).strftime('%Y-%m-%d')"/>
        </record>

        <record id="viaje_demo_madrid" model="svl_agenciaviajes.viaje">
            <field name="name">Madrid Cultural</field>
            <field name="precio_base">500.0</field>
            <field name="destino_id" ref="destino_espana"/>
            <field name="cliente_id" ref="cliente_demo_john"/>
            <field name="temporada_id" ref="temporada_baja"/>
            <field name="fecha_inicio" eval="(datetime.now() + relativedelta(days=30)).strftime('%Y-%m-%d')"/>
            <field name="fecha_fin" eval="(datetime.now() + relativedelta(days=35)).strftime('%Y-%m-%d')"/>
        </record>

        <record id="viaje_demo_nueva_york" model="svl_agenciaviajes.viaje">
            <field name="name">Luces de Nueva York</field>
            <field name="precio_base">1500.0</field>
            <field name="destino_id" ref="destino_eeuu_ny"/>
            <field name="cliente_id" ref="cliente_demo_maria"/>
            <field name="temporada_id" ref="temporada_alta"/>
            <field name="fecha_inicio" eval="(datetime.now() + relativedelta(days=90)).strftime('%Y-%m-%d')"/>
            <field name="fecha_fin" eval="(datetime.now() + relativedelta(days=95)).strftime('%Y-%m-%d')"/>
        </record>
    </data>
</odoo>