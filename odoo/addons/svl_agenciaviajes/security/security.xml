<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">

        <record id="categoria_agencia" model="ir.module.category">
            <field name="name">Agencia de Viajes</field>
            <field name="description">Nivel de acceso para el m√≥dulo de viajes</field>
        </record>

        <record id="grupo_agencia_usuario" model="res.groups">
            <field name="name">Empleado</field>
            <field name="category_id" ref="categoria_agencia"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        </record>

        <record id="grupo_agencia_manager" model="res.groups">
            <field name="name">Manager</field>
            <field name="category_id" ref="categoria_agencia"/>
            <field name="implied_ids" eval="[(4, ref('grupo_agencia_usuario'))]"/>
        </record>

    </data>
</odoo>